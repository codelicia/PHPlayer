{
  "name": "codelicia/phplayer",
  "type": "project",
  "description": "A simple music player writing in php, based on http://cmd.fm",
  "license": "MIT",
  "keywords": [
    "player",
    "console",
    "terminal"
  ],
  "homepage": "https://github.com/codelicia/PHPlayer",
  "authors": [
    {
      "name": "Jefersson Nathan",
      "email": "malukenho.dev@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "symfony/console": "^7.0",
    "laminas/laminas-http": "^2.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0.0",
    "infection/infection": "^0.29.0",
    "phpunit/phpunit": "^10.0.0",
    "roave/security-advisories": "dev-master",
    "staabm/annotate-pull-request-from-checkstyle": "^1.4.0",
    "vimeo/psalm": "4.x-dev"
  },
  "autoload": {
    "psr-4": {
      "PHPlayer\\": "src/"
    }
  }
}
